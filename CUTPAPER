#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    
    cin>>t;
    while(t--){
        int n,k;
        cin>>n>>k;
        if(n==k){
            cout<<1<<endl;
            
        }
        else if(k==1){
            cout<<n*n<<endl;
        }
        else{
            cout<<(n/k)*(n/k)<<endl;
        }
    }
    
    return 0;
}
