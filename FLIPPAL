#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string s;
        cin>>s;
        int co=0,cz=0;
        for(int i=0 ; i<n ; i++){
            if(s[i]=='1'){
                co++;
            }
            else{
                cz++;
            }
        }
        if(co%2==1 && cz%2==1){
            cout<<"no"<<endl;
        }
        
        else{
            cout<<"yes"<<endl;
        }
        
    }
    
    return 0;
}
